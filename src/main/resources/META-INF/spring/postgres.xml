<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:p="http://www.springframework.org/schema/p"
        xmlns:c="http://www.springframework.org/schema/c"
        xsi:schemaLocation="
                http://www.springframework.org/schema/beans
                http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

    <bean id="dwDataSource" class="org.apache.commons.dbcp2.BasicDataSource" destroy-method="close"
            p:driverClassName="org.postgresql.Driver"
            p:url="jdbc:postgresql://127.0.0.1:5432/dashboard"
            p:username="#{dashboardConfig.dwUsername}"
            p:password="#{dashboardConfig.dwPassword}" />

    <bean id="dwTemplate" class="org.springframework.jdbc.core.namedparam.NamedParameterJdbcTemplate"
            c:dataSource-ref="dwDataSource" />

    <bean id="dwInit" class="org.sagebionetworks.dashboard.dao.postgres.DataWarehouseInit"
            c:dwTemplate-ref="dwTemplate" />

</beans>
